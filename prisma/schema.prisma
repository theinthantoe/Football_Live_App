generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model VersionControl {
  id  String @id @default(auto()) @map("_id") @db.ObjectId
  ios Json
  android Json
}

model  Membership {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  slogan String
  telegram Json
  viber Json
  messenger Json
  facebook Json
}
model  SplashBanner {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  image String
}


